    "grunt-contrib-watch-chokidar": "1.0.0",
    "grunt-curl": "2.4.1",
    "grunt-download-electron": "2.1.4",
    "grunt-electron": "2.0.1",
    "grunt-electron-installer": "1.2.3",
    "grunt-electron-packager": "0.1.1",
    "grunt-if-missing": "1.0.1",
    "grunt-newer": "1.3.0",
