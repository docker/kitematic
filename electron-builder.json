{
  "appId": "com.docker.kitematic",
  "asar": false,
  "directories": {
    "app": "./build/",
    "output":"./dist/"
  },
  "linux":{
  },
  "mac": {
    "category": "public.app-category.developer-tools"
  },
  "msi": {
    "warningsAsErrors": false
  },
  "productName": "Kitematic",
  "win": {
    "extraResources":"./resources/**/*",
    "icon": "./util/kitematic.ico",
    "target": [
      {
        "target": "appx"
      },
      {
        "target": "msi"
      },
      {
        "target": "portable"
      }
    ]
  }
}